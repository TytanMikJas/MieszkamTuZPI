{
  "name": "mitu-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:prod": "npm run env:prod node dist/src/main",
    "start:dev": "npm run env:dev npm start run dev",
    "start:dev-watch": "dotenv -e ../.env.dev -- nest start --watch",
    "lint": "eslint \"{src,apps,libs,test,prisma}/**/*.ts\"",
    "lint-fix": "eslint --fix \"{src,apps,libs,test,prisma}/**/*.ts\"",
    "lint:format": "prettier  --log-level warn --write \"./**/*.{js,ts,md,prisma}\" ",
    "tsc:check": "tsc --noEmit",
    "test:cov": "jest --coverage --config ./jest.config.ts",
    "test": "jest --config ./jest.config.ts",
    "test:watch": "jest --config ./jest.config.ts --watch",
    "seed:dev": "npm run env:dev ts-node prisma/seed.ts",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "env:dev": "dotenv -e ../.env.dev --",
    "env:prod": "dotenv -e ../.env.prod --"
  },
  "dependencies": {
    "@azure/ai-text-analytics": "^5.1.0",
    "@golevelup/ts-jest": "^0.4.0",
    "@nestjs-modules/mailer": "^1.11.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.1.17",
    "@prisma/client": "^5.20.0",
    "@types/passport-jwt": "^4.0.0",
    "@types/proj4": "2.5.5",
    "bcrypt": "^5.1.1",
    "bcrypt-ts": "^5.0.0",
    "cache-manager": "^5.5.1",
    "cache-manager-redis-yet": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "file-type": "^19.0.0",
    "generate-passphrase": "^1.3.0",
    "gltf-pipeline": "^4.1.0",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "proj4": "2.9.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.3"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.26",
    "@nestjs/cli": "^10.4.7",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.20.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "jest": {
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "dotenv/config"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
